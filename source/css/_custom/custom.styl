// Custom styles.

@import "_suprise/ball";

// over write code color
.post code {
    background: none;
    color: $gainsboro;
    text-shadow: 0 0px #fff;
}

// post next article
.post-nav-item a {
    color: $gray-lighter;
}

// search input
.site-search input {
    border-bottom: 1px solid #fc6423;
}

// 归档
.posts-collapse .post-title a {
  color: $grey-light;
  border-bottom: none;
}